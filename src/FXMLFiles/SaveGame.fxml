<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="350.0" maxWidth="416.0" minHeight="80.0" minWidth="360.0" prefHeight="109.0" prefWidth="367.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SaveGameController">
   <center>
      <HBox alignment="TOP_CENTER" prefHeight="69.0" prefWidth="360.0" BorderPane.alignment="CENTER">
         <children>
      <Label text="Description">
               <font>
                  <Font size="14.0" fx:id="x2" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
      <TextField fx:id="txtDescription" onKeyPressed="#textFieldLimit" prefHeight="25.0" prefWidth="192.0" />
            <Button mnemonicParsing="false" onAction="#saveButtonAction" text="Save" />
         </children>
         <BorderPane.margin>
            <Insets top="20.0" />
         </BorderPane.margin>
      </HBox>
   </center>
   <bottom>
      <VBox alignment="TOP_CENTER" prefHeight="44.0" prefWidth="360.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnBack" mnemonicParsing="false" onAction="#backButtonAction" text="Back" />
         </children>
      </VBox>
   </bottom>
</BorderPane>
